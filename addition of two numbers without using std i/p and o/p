#include <stdio.h>
int add (int x,int y)
{
    int z;
    z=x+y;
    return z;
}
int main(){
    int a,b,c;
    printf("enter the the two numbers\n");
    scanf("%d\n%d",&a,&b);
    c=add(a,b);
    printf("%d",c);
    return 0;
}
